{
    "name": "quanta/http-server",
    "type": "library",
    "description": "Http server for running psr-7 applications",
    "keywords": [
        "psr-7",
        "server"
    ],
    "license": "MIT",
    "homepage": "https://github.com/quantaphp/http-server",
    "support": {
        "issues": "https://github.com/quantaphp/http-server/issues",
        "source": "https://github.com/quantaphp/http-server"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "eloquent/phony-kahlan": "^2.0",
        "kahlan/kahlan": "^4.0",
        "phpstan/phpstan": "^0.11.2"
    },
    "autoload": {
        "psr-4": {
            "Quanta\\Http\\": "src"
        }
    }
}
